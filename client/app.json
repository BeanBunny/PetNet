{
    "expo": {
        "name": "PetNet",
        "slug": "client",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/Logo1.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/Logo1.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.petnet.petnet"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
